{
	"name": "Flutter Dev Env",
	"privileged": true,
	"mounts": [
		{
			"type": "bind",
			"source": "/dev/bus/usb",
			"target": "/dev/bus/usb"
		}
	],
	"build": {
		"dockerfile": "Dockerfile"
	},
	"features": {
		"ghcr.io/devcontainers/features/java:1": {
			"version": "none",
			"installMaven": "false",
			"installGradle": "false"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"Dart-Code.flutter",
				"Dart-Code.dart-code"
			]
		}
	}
}